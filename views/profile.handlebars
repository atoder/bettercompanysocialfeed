<div class="row">
    <div class="col-md-offset-3 col-md-1">
        <a href="/dashboard"><span class="glyphicon glyphicon-arrow-left" aria-hidden="true"></span></a>
    </div>
    <div class="col-md-4">
        <div id="profile">
            <img src="/profile/images?profile={{avatar}}" alt="Profile Picture" />
            <h4 class="profile-email">{{email}}</h4>
            <div class="description-text">{{description}}</div>
        </div>
    </div>
</div>
